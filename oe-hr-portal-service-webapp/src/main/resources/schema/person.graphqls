  type Person {
    id: Int
    firstName: String
    lastName: String
    email: String
    contactId: String
    salesforcePurchaserId: String
    workingLevel: Level
 }

 type Level {
    id: ID
    name: String
    description: String
    active: Boolean
    numLiveRequired: Int
    numImmersionRequired: Int
    sequence: Int
    lowScoreBoundary: Float
    highScoreBoundary: Float
    levelNum: String
 }

 type PersonsPerLevel {
    levelName: String
    totalNumber: Int
 }

 type PersonActivitiesTotal{
    person: Person
    totalActivities: Float
 }

 type PersonsUsageLevelOverview {
    person: Person
    start: String
    expiration: String
    usageLevel: UsageLevel
    remainingDays: Int
    inactiveDays: Int
 }

 enum UsageLevel{
     HIGH
     MEDIUM_HIGH
     MEDIUM_LOW
     LOW
  }

 type License {
      person: Person
      status: String
      startDate: String
      organization: String
      name: String,
      licenseId: String,
      endDate: String,
      privateClasses: Int
  }