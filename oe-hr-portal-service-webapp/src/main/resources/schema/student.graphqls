 type Student {
    id: ID
    contactId: String
    details: StudentDetail
    workingLevel: Level
 }

 type StudentDetail {
    id: ID
    salesforcePurchaserId: Int
 }

 type Level {
    id: ID
    name: String
    description: String
    active: Boolean
    numLiveRequired: Int
    numImmersionRequired: Int
    sequence: Int
    lowScoreBoundary: Float
    highScoreBoundary: Float
    levelNum: String
 }